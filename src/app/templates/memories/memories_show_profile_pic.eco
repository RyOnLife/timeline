<li data-fb-id="<%= @friend.id %>" data-tagged-by="<%= @taggedBy %>">
  <div class="profile_pic">
    <label></label>
    <img src="http://graph.facebook.com/<%= @friend.id %>/picture?type=square" width="50" height="50" />
  </div>
  <div class="name" >
    <a href="<%= @friend.link %>"><%= @friend.name %></a>
  </div>
</li>
